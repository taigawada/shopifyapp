{
    "name": "shopify-app-node",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "debug": "node --inspect-brk index.js",
        "dev": "cross-env NODE_ENV=development nodemon -L --ignore ./frontend & graphql-codegen-esm --config codegen.yml --watch admin-graphql/documents/*.graphql",
        "serve": "prisma generate && cross-env NODE_ENV=production ts-node --esm index.js",
        "codegen": "graphql-codegen-esm --config codegen.yml"
    },
    "type": "module",
    "engines": {
        "node": ">=14.13.1"
    },
    "dependencies": {
        "@prisma/client": "^3.15.2",
        "@shopify/shopify-api": "^5.0.0",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cross-env": "^7.0.3",
        "express": "^4.17.3",
        "serve-static": "^1.14.1"
    },
    "devDependencies": {
        "@graphql-codegen/cli": "2.16.1",
        "@graphql-codegen/client-preset": "1.2.3",
        "@graphql-codegen/typescript": "^2.8.5",
        "@graphql-codegen/typescript-graphql-request": "^4.5.8",
        "@graphql-codegen/typescript-operations": "^2.5.10",
        "jsonwebtoken": "^8.5.1",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.2",
        "pretty-quick": "^3.1.3",
        "prisma": "^3.15.2",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    }
}
